@article{zhang_state-dependent_2019,
	title = {State-{Dependent} {Spike} and {Local} {Field} {Synchronization} between the {Thalamic} {Parafascicular} {Nucleus} and the {Dorsal} {Striatum} in a {Rat} {Model} of {Parkinson}'s {Disease}},
	volume = {404},
