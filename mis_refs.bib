	author = {Garrido, Jesús A. and Luque, Niceto R. and D'Angelo, Egidio and Ros, Eduardo},
	year = {2013},
	file = {Garrido et al. - 2013 - Distributed cerebellar plasticity implements adapt.pdf:/Users/mq20185996/Zotero/storage/BV2NG6EG/Garrido et al. - 2013 - Distributed cerebellar plasticity implements adapt.pdf:application/pdf}
